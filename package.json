{
  "name": "tellytax",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "build:css": "tailwindcss -i src/styles.css -o public/style.css --minify",
    "build:js": "esbuild src/client.tsx --bundle --outfile=public/client.js --format=esm --target=es2020 --jsx=automatic",
    "build": "pnpm build:css && pnpm build:js",
    "dev": "pnpm build && wrangler dev",
    "deploy": "pnpm build && wrangler deploy",
    "types": "wrangler types"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.39.0",
    "@radix-ui/react-collapsible": "^1.1.3",
    "@radix-ui/react-slot": "^1.1.2",
    "@radix-ui/react-switch": "^1.1.3",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "lucide-react": "^0.474.0",
    "marked": "^17.0.2",
    "partyserver": "^0.1.5",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "resend": "^4.1.0",
    "tailwind-merge": "^3.0.1",
    "y-partyserver": "^1.0.0",
    "yjs": "^13.6.29"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20250214.0",
    "@tailwindcss/cli": "^4.0.0",
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.0.0",
    "esbuild": "^0.27.3",
    "tailwindcss": "^4.1.18",
    "typescript": "^5.7.3",
    "wrangler": "^3.109.2"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "esbuild",
      "workerd"
    ]
  }
}